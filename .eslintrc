{
    "env": {
        "node": true
    },
    "extends": ["airbnb-base", "prettier"],
    "plugins": ["prettier"],
    "rules": {
        "indent": ["error", 4, { "SwitchCase": 1 }],
        "no-var": 0,
        "object-shorthand": 0,
        "arrow-body-style": 0,
        "comma-dangle": 0,
        "no-console": 0,
        "no-plusplus": 0,
        "func-names": 0,
        "no-underscore-dangle": 0,
        "max-len": ["error", 140],
        "import/no-unresolved": [0, { "commonjs": true, "amd": true }]
    }
}
